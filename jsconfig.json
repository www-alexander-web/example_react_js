{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@starter": [
        "src/Starter"
      ],
      "@starter/*": [
        "src/Starter/*"
      ],
      "@base": [
        "src/Base"
      ],
      "@base/*": [
        "src/Base/*"
      ],
      "@auth": [
        "src/Authorization"
      ],
      "@auth/*": [
        "src/Authorization/*"
      ],
      "@mg": [
        "src/Monitoring"
      ],
      "@mg/*": [
        "src/Monitoring/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}